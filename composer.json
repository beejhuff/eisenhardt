{
    "name": "maxbucknell/eisenhardt",
    "description": "Batteries not included Docker environment for Magento 2.",
    "keywords": [ "magento", "magento2", "docker", "docker-compose" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Max Bucknell",
            "email": "me@maxbucknell.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MaxBucknell\\": "src/MaxBucknell/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxBucknell\\": "test/MaxBucknell/"
        }
    },
    "require": {
        "symfony/console": "^4.1",
        "symfony/filesystem": "^4.1"
    },
    "bin": [
        "eisenhardt"
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "scripts": {
        "test": "phpunit --bootstrap='vendor/autoload.php' test --color=always"
    }
}
